<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="WhackAMonkey.AboutPage"
             xmlns:ef="clr-namespace:WhackAMonkey.Effects;assembly=WhackAMonkey"
             Title="About">
  <StackLayout>
    <Label Text="{Binding About}" VerticalOptions="Center" HorizontalOptions="Center" FontSize="20">
       <Label.Effects>
        <ef:LabelEffect/>
      </Label.Effects>
    </Label>
    
  </StackLayout>
  
</ContentPage>